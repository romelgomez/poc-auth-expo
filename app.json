{
  "expo": {
    "name": "poc-expo-auth",
    "slug": "poc-expo-auth",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "demo",
    "android": {
      "package": "com.pocexpoauth.demo",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json"
    }
  }
}
